2.
CREATE TABLESPACE UBER DATAFILE
'UBER_1' SIZE 2G;

CREATE UNDO TABLESPACE UNDO_TBS DATAFILE
'UNDO_UBER' SIZE 25M;

CREATE BIGFILE TABLESPACE UBER DATAFILE
'BIG_UBER' SIZE 5G;


ALTER SYSTEM SET UNDO_TABLESPACE = UNDO_TBS;

4.
CREATE PROFILE CLERK LIMIT
PASSWORD_LIFE_TIME 40
SESSIONS_PER_USER 1
IDLE_TIME 30
FAILED_LOGIN_ATTEMPTS 4;

CREATE PROFILE DEVELOPMENT LIMIT
PASSWORD_LIFE_TIME 100
SESSIONS_PER_USER 2
IDLE_TIME 10
FAILED_LOGIN_ATTEMPTS UNLIMITED;

5.

CREATE USER UBER_1 IDENTIFIED BY 12345 DEFAULT TABLESPACE UBER QUOTA UNLIMITED ON UBER;
GRANT CONNECT TO UBER_1;
GRANT CLERK TO UBER_1;

CREATE USER UBER_2 IDENTIFIED BY 12345 DEFAULT TABLESPACE UBER QUOTA UNLIMITED ON UBER;
GRANT CONNECT TO UBER_2;
GRANT CLERK TO UBER_2;

CREATE USER UBER_3 IDENTIFIED BY 12345 DEFAULT TABLESPACE UBER QUOTA UNLIMITED ON UBER;
GRANT CONNECT TO UBER_3;
GRANT DEVELOPMENT TO UBER_3;

CREATE USER UBER_4 IDENTIFIED BY 12345 DEFAULT TABLESPACE UBER UNLIMITED ON UBER;
GRANT CONNECT TO UBER_4;
GRANT DEVELOPMENT TO UBER_4;


ALTER USER UBER_1 ACCOUNT LOCK;
